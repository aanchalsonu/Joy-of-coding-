//program to find sum of terms 

#include <stdio.h>

int main()
{
    int rem,num,sum=0,rem2,a=0;
    printf("Enter a number ");
    scanf("%d",&num);
    while(num!=0)
    {
        rem=num%10;
        sum=sum+rem;
        num=num/10;
    }
    
    while(sum!=0)
    {
        rem2=sum%10;
        a=a+rem2;
        sum=sum/10;
    }
    printf("The sum of digits is %d ",a);
    return 0;
}
